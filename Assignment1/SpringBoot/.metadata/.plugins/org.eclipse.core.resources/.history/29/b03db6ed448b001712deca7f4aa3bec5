import java.util.*;

import org.springframework.stereotype.Repository;

@Repository
public class MemberData {
	
private static Map<Integer, Member>MemberList;
	
     static {
    	 
    	 MemberList = new HashMap<Integer, Member>() {
    		 {
    			 put(1,new Member(1, "Gold","John"));
    			 put(2,new Member(2, "Silver","Tom"));
    			 put(3,new Member(1, "Platinum","Sarah"));
    		 }
    		 
    	 };
		
	}


}
